<nav class="d-flex justify-content-center" aria-label="Page navigation example">
    <ul class="pagination">
      <li class="page-item">
        {% if paginate.previous %}
          <a class="page-link" href="{{ paginate.previous.url }}" rel="previous">Next</a>
        {% else %}
          <a class="page-link disabled" href="#" rel="previous">Previous</a>
        {% endif %}
      </li>

      {% for part in paginate.parts %}
        {% if part.is_link %}
          <li class="page-item">
            <a class="page-link" href="{{ part.url }}" title="">{{ part.title }}</a>
          </li>
        {% else %}
          {% if part.title == paginate.current_page %}
            <li class="page-item active"><a class="page-link disabled" href="#">{{ part.title }}</a></li>
          {% else %}
            <li class="page-item"><a class="page-link disabled" href="#">{{ part.title }}</a></li>
          {% endif %}
        {% endif %}
      {% endfor %}

      <li class="page-item">
        {% if paginate.next %}
          <a class="page-link" href="{{ paginate.next.url }}" rel="next">Next</a>
        {% else %}
          <a class="page-link disabled" href="#" rel="next">Next</a>
        {% endif %}
      </li>
    </ul>
</nav>